justification pattern prover_pattern {

    @support Su1 is "Model is ready"
    @support Su2 is "Prover is available"
    evidence Su3 is "Scenario are available"

    strategy St1 is "Prove model"
    Su1 supports St1
    Su2 supports St1
    Su3 supports St1

    conclusion C is "Model is correct"
    St1 supports C

}

justification J1 realizes prover_pattern {

    override sub-conclusion Su1 = "Merged model is available"
    strategy St0 = "Merge Atomic model"
    support Su0 = "Atomic Petri nets are available"
    Su0 supports St0
    St0 supports Su1

    override support Su2 = "TINA is available"

}

